<ul>
    {{#each modules as |module|}}
    <li>
        {{#link-to module.path title=module.title  }}
          <i class={{module.icon}} aria-hidden="true"></i>
        {{/link-to}}
    </li>
    {{/each}}
</ul>
<ul id='logout'>
    <li>
        <a href="#" title='Sign Out' {{action 'logOut'}}>
            <i class="fa fa-sign-out" aria-hidden="true"></i>
        </a>
    </li>
</ul>
